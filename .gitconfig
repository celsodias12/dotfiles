[init]
	defaultBranch = main
[alias]
  sw = switch
	swc = switch -c
  cm = commit
  cmm = commit -m
  br = branch
	brd = branch -d
	brr = branch -m
	a = add
	re = reset
	st = stash
	stl = stash list
	sts = stash save
	sta = stash apply
	std = stash drop
	stp = stash pop
	lg = log --graph
	cmin = cmm 'feat: inital commit'
	cl = clone
	puor = pull origin
	cmam = commit --amend -m
	delcm = reset HEAD~1
	fbrs = fetch --all
	me = merge
	meq = merge --quit
	reh = reset --hard
	exst = !git add . && git stash && git stash show "stash@{0}" -p > stash.patch

[includeIf "gitdir:~/Desktop/projects/work/b2/"]
  path = .gitconfig.b2

[includeIf "gitdir:~/Desktop/projects/work/luizalabs/"]
  path = .gitconfig.luizalabs

[includeIf "gitdir:~/Desktop/projects/work/b2/tools-b2list/"]
	path = .gitconfig.default
