{
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.tabs.enabled": true,

  "workbench.colorTheme": "Dracula Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.labelFormat": "short",
  "workbench.sideBar.location": "left",
  "workbench.colorCustomizations": {
    "errorLens.infoBackground": "#66000000",
    "errorLens.warningBackground": "#66000000",
    "errorLens.infoForeground": "#737373",
    "errorLens.warningForeground": "#a0734b",
    "errorLens.errorBackground": "#66000000",
    "errorLens.hintBackground": "#66000000"
  },

  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.lineHeight": 0,
  "editor.renderIndentGuides": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [80, 110],
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },

  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/package-lock.json": true
  },

  "prettier.bracketSpacing": true,
  "prettier.jsxBracketSameLine": false,
  "prettier.trailingComma": "es5",
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.arrowParens": "avoid",
  "prettier.printWidth": 80,

  "files.autoSave": "afterDelay",
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact"
  },
  "files.eol": "\n",

  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  "javascript.suggest.autoImports": true,

  "typescript.suggest.autoImports": true,

  "screencastMode.onlyKeyboardShortcuts": true,

  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "domain": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "websockets": "pipe",
    "protos": "pipe",
    "grpc": "pipe",
    "providers": "include",
    "subscribers": "messages",
    "useCases": "controller",
    "kafka": "scripts",
    "mappers": "meta",
    "_shared": "shared",
    "eslint-config": "tools",
    "kube": "kubernetes"
  },
  "material-icon-theme.files.associations": {
    "ormconfig.json": "database",
    "tsconfig.json": "tune",
    "*.proto": "3d",
    "*.webpack.js": "webpack"
  },

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[prisma]": {
    "editor.formatOnSave": true
  },

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "MikeBovenlander.formate"
  },

  "redhat.telemetry.enabled": false,

  "quokka.compactMessageOutput": false,

  "cSpell.userWords": [
    "Ionicons",
    "Parens",
    "RECENTLOGGED",
    "REPORTSELECTED",
    "RNFS",
    "Xiaomi",
    "appcenter",
    "avds",
    "browserslist",
    "categorys",
    "celso",
    "checkmark",
    "codepush",
    "datetimepicker",
    "formik",
    "gradlew",
    "middlewares",
    "navs",
    "noopener",
    "noreferrer",
    "persistor",
    "pjpeg",
    "reactotron",
    "signin",
    "singin",
    "soapenv",
    "submenu"
  ],
  "sonarlint.ls.javaHome": "C:\\Program Files\\Java\\jdk1.8.0_291",

  "tabnine.experimentalAutoImports": true,

  "sonarlint.rules": {
    "javascript:S4326": {
      "level": "off"
    },
    "javascript:S3358": {
      "level": "off"
    }
  },
  "liveshare.connectionMode": "relay",
  "yaml.schemas": {
    "file:///c%3A/Users/celso/.vscode/extensions/atlassian.atlascode-2.9.1/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  }
}
