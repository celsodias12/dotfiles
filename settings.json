    "route.js": "Routing",
    "route.jsx": "Routing",
    "routes.ts": "Routing",
    "routes.tsx": "Routing",
    "routes.js": "Routing",
    "routes.jsx": "Routing",
    "router.ts": "Routing",
    "router.tsx": "Routing",
    "router.js": "Routing",
    "router.jsx": "Routing",
    "*.route.ts": "Routing",
    "*.route.tsx": "Routing",
    "*.route.js": "Routing",
    "*.route.jsx": "Routing",
    "*.routes.ts": "Routing",
    "*.routes.tsx": "Routing",
    "*.routes.js": "Routing",
    "*.routes.jsx": "Routing",
    ".i18n.js": "I18n",
    ".i18n.ts": "I18n",
    "types.ts": "typescript-def",
    "type.ts": "typescript-def",
    "*.types.ts": "typescript-def",
    "*.type.ts": "typescript-def"
  },

