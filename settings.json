{
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "terminal.integrated.tabs.enabled": true,

  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Dracula Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.labelFormat": "short",
  "workbench.sideBar.location": "right",
  "workbench.colorCustomizations": {
    "errorLens.infoBackground": "#66000000",
    "errorLens.warningBackground": "#66000000",
    "errorLens.infoForeground": "#737373",
    "errorLens.warningForeground": "#a0734b",
    "errorLens.errorBackground": "#66000000",
    "errorLens.hintBackground": "#66000000",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },

  "explorer.compactFolders": false,

  "editor.bracketPairColorization.enabled": true,
  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.lineHeight": 0,
  "editor.renderIndentGuides": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderLineHighlight": "gutter",
  "editor.rulers": [90, 120],
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.inlineSuggest.enabled": true,
  "editor.wordWrap": "off",
  "editor.wordBasedSuggestions": false,
  "editor.suggest.preview": true,

  "search.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true
  },

  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,

  "prettier.bracketSpacing": true,
  "prettier.jsxBracketSameLine": false,
  "prettier.trailingComma": "es5",
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.arrowParens": "avoid",
  "prettier.printWidth": 80,
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.insertPragma": false,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "preserve",
  "prettier.requirePragma": false,
  "prettier.semi": false,
  "prettier.useTabs": false,
  "prettier.endOfLine": "auto",

  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact"
  },

  "emmet.syntaxProfiles": { "javascript": "jsx" },
  "emmet.includeLanguages": { "javascript": "javascriptreact" },

  "screencastMode.onlyKeyboardShortcuts": true,

  "material-icon-theme.activeIconPack": "nest",
