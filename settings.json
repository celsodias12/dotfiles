{
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.shellIntegration.history": 1000,
  "terminal.integrated.env.linux": {},
  "terminal.integrated.env.windows": {},

  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Dracula Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.labelFormat": "short",
  "workbench.editor.splitInGroupLayout": "horizontal",
  "workbench.editor.historyBasedLanguageDetection": true,
  "workbench.colorCustomizations": {
    "errorLens.infoBackground": "#66000000",
    "errorLens.warningBackground": "#66000000",
    "errorLens.infoForeground": "#737373",
    "errorLens.warningForeground": "#a0734b",
    "errorLens.errorBackground": "#66000000",
    "errorLens.hintBackground": "#66000000",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "workbench.list.smoothScrolling": true,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.editor.pinnedTabsOnSeparateRow": true,

  "explorer.compactFolders": false,
  "explorer.autoRevealExclude": {
    "**/node_modules": true
  },

  "editor.tabSize": 2,
  "editor.fontSize": 14,
  "editor.rulers": [120],
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.lineHeight": 0,
  // "editor.formatOnSave": true,
  // "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderLineHighlight": "all",
  "editor.linkedEditing": true,
  "editor.suggestSelection": "first",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.addMissingImports": true
  },
  "editor.suggest.showMethods": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.wordWrap": "off",
  "editor.wordBasedSuggestions": false,
  "editor.suggest.preview": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": "active",
  "editor.bracketPairColorization.enabled": true,
  "editor.accessibilitySupport": "off",
  "editor.hover.above": true,
  "editor.smoothScrolling": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "editor.unicodeHighlight.allowedCharacters": {
    "ê": true,
    "ç": true,
    "ã": true,
    "à": true,
    "á": true,
    "õ": true,
    "ú": true,
    "º": true,
    "Ç": true,
    "Ã": true,
    "ó": true,
    "í": true,
    "é": true,
    "â": true,
    "Ô": true,
    "É": true,
    "°": true,
    "ñ": true,
    "¡": true,
    "¿": true,
    "©": true,
    "Ó": true,
    "Ú": true,
    "Á": true,
    "ü": true,
    "û": true,
    "Í": true,
    "❯": true,
    "’": true,
    "ô": true
  },
  "editor.stickyScroll.enabled": true,
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "editor.minimap.autohide": true,
  "editor.minimap.showSlider":"mouseover",
  "editor.renderWhitespace": "trailing",
  "editor.inlayHints.enabled": "on",
  "editor.codeActionWidget.includeNearbyQuickfixes": true,

  "search.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**.min.js": true
  },

  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.inlayHints.parameterNames.enabled": "literals",
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": false,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "javascript.suggest.classMemberSnippets.enabled": true,
  "javascript.validate.enable": true,
  "javascript.autoClosingTags": true,
  "javascript.referencesCodeLens.enabled": true,

  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": false,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": true,
  "typescript.suggest.classMemberSnippets.enabled": true,
  "typescript.validate.enable": true,
  "typescript.autoClosingTags": true,
  "typescript.referencesCodeLens.enabled": true,

  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.quoteProps": "as-needed",
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.bracketSameLine": true,
  "prettier.arrowParens": "avoid",
  "prettier.proseWrap": "preserve",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.endOfLine": "lf",
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.singleAttributePerLine": false,

  "files.associations": {
    "*.tsx": "typescriptreact",
    ".env*": "dotenv"
  },
  "files.trimTrailingWhitespace": true,
  // "files.readonlyInclude": {
  //   "node_modules/*": true
  // },
  "files.readonlyFromPermissions": true,
  "files.exclude": {
    "**/.git": true
  },

  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "screencastMode.onlyKeyboardShortcuts": true,
  "screencastMode.fontSize": 26,

  "npm.packageManager": "npm",

  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
